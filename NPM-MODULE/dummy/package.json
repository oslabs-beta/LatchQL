{
  "name": "root-latch",
  "version": "1.0.0",
  "description": "a one stop shop for securing a graphQL API",
  "type": "module",
  "main": "server.js",
  "types": "server.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run build",
    "start": "run-s kill-redis start-redis backend",
    "backend": "ttab -t 'Running server' node server.js",
    "kill-redis": "ttab -t 'Killing preexisting redis server' killall redis-server",
    "start-redis": "ttab -t 'Running redis server' redis-server",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcphail-alex/LatchQL.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "graphQL",
    "depth",
    "limit",
    "rate",
    "limit",
    "cost",
    "limit"
  ],
  "author": "Celine Leung, Hannah Bernstein, Ray Kim, Johnjered Tolentino, Alex McPhail",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mcphail-alex/LatchQL/issues"
  },
  "homepage": "https://github.com/mcphail-alex/LatchQL#readme",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "@types/node": "^16.11.68",
    "babel-jest": "^29.2.2",
    "body-parser": "^1.20.1",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ttab": "^0.7.3"
  },
  "dependencies": {
    "@graphql-codegen/typescript-resolvers": "^2.7.5",
    "@graphql-tools/schema": "^9.0.4",
    "@types/jsonwebtoken": "^8.5.9",
    "apollo-server-express": "^3.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-http-proxy": "^1.6.3",
    "express-jwt": "^7.7.5",
    "fakebase": "^0.3.0",
    "find-config": "^1.0.0",
    "fs": "^0.0.1-security",
    "graphql": "^15.8.0",
    "graphql-middleware": "^6.1.33",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "latchql": "file:../lib",
    "monaco-editor": "^0.34.1",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "redis": "^4.3.1",
    "redis-server": "^1.2.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
